<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.toy.pojo.Manager" table="manager" schema="toy">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property name="managerName" type="java.lang.String"  column="managerName"/>
        <property name="managerPassword" type="java.lang.String" column="managerPassword"/>
    </class>
</hibernate-mapping>